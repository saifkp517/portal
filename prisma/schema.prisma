generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model property {
  id                 String   @id @default(uuid())
  building_name      String
  asset_type         String
  investment_size    String
  lockin             String
  entry_yeild        String
  irr                String
  multiplier         String
  minimum_investment String
  location           String
  tenant             String
  overview           String
  floorplan          Json?
  tenant_details     Json?
  images             String?
  additional         Json?
}

model user {
  id       String  @id @default(uuid())
  name     String?
  email    String  @unique
  phone    String  @unique
  password String
}
